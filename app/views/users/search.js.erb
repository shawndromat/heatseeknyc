<% if current_user.is_demo_user? %>
  $(".alert").remove();
  $("#search-results").remove();
  $(".search-results-container").html('<%= j render "demo_search_results" %>');
<% else %>
  $(".alert").remove();
  $("#search-results").remove();
  $(".search-results-container").html('<%= j render "search_results" %>');
<% end %>
