<br>
<div class="col-xs-6 container" id="search-results">
  <h1 class="styled-header" class="text-center">Results</h1>
  <ul>
    <% @results.each do |r| %>
      <li class="sidebar-li">
        <hr class="underline-user text-center">
        <a href="/users/<%= r.id %>" class="user-link"><%= r.first_name %> <%= r.last_name %></a>
        <%= link_to "",
          user_collaborations_path(current_user, collaborator_id: r),
          method: :post,
          class: "fa fa-plus add-user-link",
          title: "Add Tenant"
        %>
        <br>
      </li>
    <% end %>
  </ul>
</div>
