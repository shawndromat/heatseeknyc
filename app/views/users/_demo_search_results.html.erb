<div class="col-xs-12 container" id="search-results">
  <h1 class="styled-header" class="text-center">Results</h1>
  <ul id="collaborator-ul" class="search-results-ul">
    <% @results.each do |r| %>
      <li class="sidebar-li">
        <% if User.account_demo_user?(r) %>
          <a href="/users/<%= r.id %>" class="user-link"><%= r.first_name %> <%= r.last_name %></a>
          <p class="address_zip"><%= "#{r.address}, #{r.zip_code}" %></p>
          <%= link_to "", user_collaborations_path(current_user, collaborator_id: r), method: :post, :class => "fa fa-plus add-user-link" %>
          <br>
        <% else %>
          <a href="#" class="user-link"><%= r.first_name %></a>
          <p class="address_zip"><%= "#{r.address}, #{r.zip_code}" %></p>
          <a href="#" class="fa fa-plus add-user-link"></a>
          <br>
        <% end %>

      </li>
    <% end %>
    
  </ul>
</div>